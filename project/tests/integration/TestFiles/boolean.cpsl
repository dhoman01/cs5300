CONST a = true;
      b = false;

VAR c    : boolean;
    d, e : integer;

BEGIN

write("Beginning Program");
write("-----------------\n");

d := 10;
e := 20;

write("a=true (1) and b=false(0)");
write("d=10 and e=20");

write("\nc := a & b\n");
c := a & b;   $false  (0)
write("c = ", c);

write("\nc := a | b\n");
c := a | b;   $true   (1)
write("c = ", c);

write("\nc := d > e\n");
c := d > e;   $false   (0)
write("c = ", c);

write("\nc := d >= e\n");
c := d >= e;  $false  (0)
write("c = ", c);

write("\nc := d < e\n");
c := d < e;   $true   (1)
write("c = ", c);

write("\nc := d <= e\n");
c := d <= e;  $true   (1)
write("c = ", c);

write("\nc := d = e\n");
c := d = e;   $false  (0)
write("c = ", c);

write("\nc := d <> e\n");
c := d <> e;  $true   (1)
write("c = ", c);

write("\nc := ~a\n");
c := ~a;      $false  (0)
write("c = ", c);

write("\n--------------");
write("Ending Program");

END.