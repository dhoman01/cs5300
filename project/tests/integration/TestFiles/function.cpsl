var i : integer;

function fib(var n : integer) : integer;
begin
    if n = 0 then
        return 0;
    end;
    if n = 1 then
        return 1;
    end;
    return fib(n - 1) + fib(n - 2);
end;

begin
    write("Enter ith: ");
    read(i);
    write("You entered: ", i, "\n");
    write("Fib of ", i, " is ");
    write(fib(i), "\n");
end.