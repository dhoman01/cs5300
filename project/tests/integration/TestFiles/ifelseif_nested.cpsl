VAR 
  i, j : integer;

BEGIN

  i := 3;
  j := 1;

  if (i < 5)
    then write("i less than 5!\n");
    if (j = 1)
      then write("j is 1\n");
    end;
  elseif (i = 5)
    then write("i is 5!");
  elseif (i < 10)
    then write("i is greater than 5, but less than 10!");
  else
    write("i is 10!");
  end;

END.